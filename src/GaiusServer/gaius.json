{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "GaiusEngine" : {
    "SourceDirectoryName" : "_source",
    "GenerationDirectoryName" : "_generated",
    "ThemesDirectoryName" : "_themes",
    "PostsDirectoryName" : "_posts",
