    "Pagination" : 5,
    "GenerationUrlRootPrefix" : "",
    "AlwaysKeep" : [
        ".git"
    ],
    "Worker" : "Gaius.Core.Worker.MarkdownLiquid.MarkdownLiquidWorker"
  }
}