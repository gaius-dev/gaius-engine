    "version": "0.2.0",
    "configurations": [
        {
            "name": "Build and Serve gaius-docs",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Gaius/bin/Debug/net5.0/gaius.dll",
            "args": [ "serve", "../../../gaius-docs", "--yes"],
            "cwd": "${workspaceFolder}/src/Gaius",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/GaiusServer/Views"
            }
        },
